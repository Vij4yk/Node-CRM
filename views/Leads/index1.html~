      document.observe("dom:loaded", function() {
        new crm.Popup({
          trigger     : "jumper",
          target      : "jumpbox",
          under       : "welcome",
          appear      : 0.3,
          fade        : 0.3,
          before_show : function() {
            $("jumper").className = "selected";
            $("jumpbox_label").innerHTML = ""
            $("jumpbox_label").hide();
            $("jumpbox_menu").show();
            crm.auto_complete("leads");
          },
          after_show  : function() {
            $("auto_complete_query").focus();
          },
          after_hide  : function() {
            $("jumper").className = "";
          }
        });
      });
      
    </script>
  </body>
</html>

